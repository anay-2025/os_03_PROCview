#include <stdio.h>
#include <stdlib.h>

int main() {
    FILE *fp1, *fp2, *fp3;
    char ch;

    // Run system commands
    system("ps aux > x1.txt");
    system("ps -eLf > x2.txt");

    // Open files
    fp1 = fopen("x1.txt", "r");
    fp2 = fopen("x2.txt", "r");
    fp3 = fopen("merged.txt", "w");

    if (fp1 == NULL || fp2 == NULL || fp3 == NULL) {
        printf("Error opening files\n");
        return 1;
    }
