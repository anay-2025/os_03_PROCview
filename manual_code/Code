#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <fcntl.h>
#include <sys/wait.h>

int main() {
    int fd1 = open("x1.txt", O_CREAT | O_WRONLY | O_TRUNC, 0644);
    if (fork() == 0) {
        dup2(fd1, STDOUT_FILENO);
        execlp("ps", "ps", "aux", NULL);
        exit(1);
    }
    wait(NULL);
    close(fd1);

    
